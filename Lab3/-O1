        test    rdi, rdi
        jle     .L4
        add     rdi, 1
        mov     eax, 1
        movsd   xmm1, QWORD PTR .LC0[rip]
        movapd  xmm0, xmm1
.L3:
        mulsd   xmm1, xmm3
        pxor    xmm2, xmm2
        cvtsi2sd        xmm2, rax
        divsd   xmm1, xmm2
        addsd   xmm0, xmm1
        add     rax, 1
        cmp     rax, rdi
        jne     .L3
        ret
.L4:
        movsd   xmm0, QWORD PTR .LC0[rip]
        ret
.LC1:
        .string "my_exp = "
